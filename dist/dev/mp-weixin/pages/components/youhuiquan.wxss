.mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1100;
  background-color: rgba(0, 0, 0, 0.6);
}
.btn {
  width: 70%;
  margin: 20rpx auto;
  margin-top: 40rpx;
  padding: 20rpx 0px;
  text-align: center;
  background: #4884FF;
  border-radius: 34px;
  font-size: 30rpx;
  color: #fff;
}
.wxbox {
  position: absolute;
  top: 20%;
  z-index: 998;
  width: 80%;
  left: 10%;
  background-color: #fff;
  border-radius: 30rpx;
  padding: 30rpx 0rpx;
}
.wxbox .close {
  position: absolute;
  top: 5px;
  right: 10px;
  z-index: 999;
  text-align: center;
}
.wxbox .weixin {
  margin: 20px 0 0 0px;
  text-align: center;
}
.wxbox .weixin image {
  width: 100px;
  height: 100px;
}
.wxbox .tips {
  padding: 10rpx 20rpx;
  text-align: center;
  font-size: 35rpx;
  color: #333;
}
.redbox {
  position: absolute;
  top: 20%;
  z-index: 1200;
  width: 80%;
  left: 10%;
  padding: 10rpx 0rpx;
  height: 750rpx;
}
.redbox .no1 {
  position: absolute;
  width: 100%;
  height: 100%;
}
.redbox .no2 {
  position: absolute;
  width: 80%;
  top: 150rpx;
  left: 10%;
  height: 300rpx !important;
}
.redbox .no3 {
  position: absolute;
  width: 80%;
  top: 500rpx;
  left: 10%;
  height: 100rpx !important;
}
.redbox .no4 {
  position: absolute;
  width: 15%;
  top: 660rpx;
  left: 42%;
  height: 80rpx !important;
}
.redbox .close {
  text-align: center;
}
.redbox .wrapBox {
  position: absolute;
  width: 60%;
  left: 20%;
  top: 320rpx;
  padding: 10rpx 0rpx;
  text-align: center;
}
.redbox .wrapBox .tips {
  font-size: 35rpx;
  color: #fff;
  font-weight: bold;
}
.redbox .wrapBox .tips text {
  font-weight: normal;
}
.redbox .wrapBox .money {
  font-size: 35rpx;
  color: #fff;
  margin-top: 30rpx;
  font-weight: bold;
}
.redbox .wrapBox .money text {
  font-size: 50rpx;
}
.redbox .wrapBox .btn {
  width: 80%;
  margin: 20rpx auto;
  margin-top: 40rpx;
  padding: 20rpx 0px;
  text-align: center;
  background: -webkit-linear-gradient(top, #FFF04D, #FFE06B);
  background: linear-gradient(to bottom, #FFF04D, #FFE06B);
  border-radius: 34px;
  font-size: 30rpx;
  color: #CF2A2A;
  font-weight: bold;
}
.messBox {
  position: absolute;
  top: 12%;
  z-index: 998;
  width: 80%;
  left: 10%;
  padding: 10rpx 0rpx;
}
.messBox .wrapBox {
  background-color: #fff;
  border-radius: 20rpx;
  padding: 30rpx;
}
.messBox .wrapBox .title {
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
}
.messBox .wrapBox .desc {
  height: 400rpx;
  margin-top: 20rpx;
  font-size: 30rpx;
}
.updateBox {
  position: absolute;
  top: 20%;
  z-index: 998;
  height: 300rpx;
  width: 80%;
  left: 10%;
}
.updateBox .tubiao {
  text-align: center;
}
.updateBox .tubiao image {
  z-index: 999;
  width: 200rpx;
  height: 130rpx;
}
.updateBox .wrapbox {
  position: relative;
  z-index: 998;
  top: -70rpx;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 30rpx 40rpx;
}
.updateBox .wrapbox .version {
  margin-top: 50rpx;
  font-size: 35rpx;
  color: #333;
  font-weight: bold;
  text-align: center;
}
.updateBox .wrapbox .desc-t {
  margin-top: 30rpx;
  font-size: 32rpx;
  font-weight: bold;
}
.updateBox1 {
  position: absolute;
  top: 15%;
  z-index: 998;
  width: 80%;
  left: 10%;
  padding: 10rpx;
  height: 700rpx;
}
.updateBox1 image {
  width: 100%;
  height: 100%;
}
.updateBox1 .wrapBox {
  position: absolute;
  width: 85%;
  left: 50rpx;
  top: 40rpx;
  padding: 10rpx 20rpx;
}
.updateBox1 .wrapBox .version {
  font-size: 35rpx;
  color: #fff;
}
.updateBox1 .wrapBox .desc-t {
  margin-top: 100rpx;
  font-size: 32rpx;
  font-weight: bold;
}
.desc {
  height: 230rpx;
  overflow: scroll;
  margin-top: 10rpx;
  font-size: 30rpx;
  color: #666;
  line-height: 50rpx;
}
.btnbox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  margin: 40rpx 0rpx 20rpx 0rpx;
  margin-top: 50rpx;
}
.btnbox .btnno {
  padding: 10rpx 30rpx;
  background-color: #f8f8f8;
  border-radius: 10rpx;
  font-size: 30rpx;
  color: #666;
}
.btnbox .btnUp {
  padding: 10rpx 30rpx;
  background-color: #4884FF;
  border-radius: 10rpx;
  font-size: 30rpx;
  color: #fff;
}

